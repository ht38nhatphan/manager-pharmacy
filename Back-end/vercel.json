{
  "version": 2,
  "builds": [
    { 
      "src": "*.js", 
      "use": "@vercel/node"
      
    }
  ],
  "routes": [

    { 
    "src": "/.*", 
    "dest": "/index.js"
    }
    
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "scripts": {
    "build": "npm run watch",
    "start": "node src/index.js"
  }
}